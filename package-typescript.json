{
  "name": "@h1deya/json-schema-to-zod",
  "version": "0.1.0",
  "description": "Converts JSON schema objects into Zod schemas - ESM-compatible fork of @n8n/json-schema-to-zod",
  "type": "module",
  "main": "./dist/esm/index.js",
  "exports": {
    ".": "./dist/esm/index.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:ts": "tsc -p tsconfig.json",
